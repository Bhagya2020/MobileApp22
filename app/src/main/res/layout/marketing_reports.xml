<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFF"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/hedingdesign"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:fontFamily="@font/arial"
        android:gravity="center_vertical"
        android:text="INFORMATION'S"
        android:textColor="#FFFF"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/textView6"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="60dp"
        android:fontFamily="@font/arial"
        android:gravity="center"
        android:text="Reporting Data"
        android:textColor="#520066"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="10dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/fildesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:text="Process Date "
                android:textColor="#FFF"
                android:textSize="13dp"
                app:textAllCaps="true" />

            <TextView
                android:id="@+id/txtProcessData"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/edittextdesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#520066"
                android:textSize="13dp"
                app:textAllCaps="true" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="35dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/fildesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:text="Target"
                android:textColor="#FFF"
                android:textSize="13dp"
                app:textAllCaps="true" />

            <TextView
                android:id="@+id/mtarget"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/edittextdesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#520066"
                android:textSize="13dp"
                app:textAllCaps="true" />


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="35dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/fildesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:text="Achievement"
                android:textColor="#FFF"
                android:textSize="13dp"
                app:textAllCaps="true" />

            <TextView
                android:id="@+id/mAcvment"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/edittextdesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#520066"
                android:textSize="13dp"
                app:textAllCaps="true" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="35dp">

            <TextView
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/fildesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:text="Ratio"
                android:textColor="#FFF"
                android:textSize="13dp"
                app:textAllCaps="true" />

            <TextView
                android:id="@+id/mRatio"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_weight="1"
                android:background="@drawable/edittextdesign"
                android:fontFamily="@font/arial"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                android:textColor="#520066"
                android:textSize="13dp"
                app:textAllCaps="true" />

        </TableRow>

    </LinearLayout>

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="407dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout11">

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <com.highsoft.highcharts.core.HIChartView
                android:id="@+id/hc"
                android:layout_width="match_parent"
                android:layout_height="350dp" />

        </LinearLayout>
    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>